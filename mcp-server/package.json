{
  "name": "echotune-mcp",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "start:health": "node health.js",
    "health": "curl -fsS http://localhost:${MCP_PORT:-3001}/health || exit 1"
  },
  "servers": {
    "mcpHealth": {
      "command": "node",
      "args": [
        "health.js"
      ],
      "port": 3001,
      "healthPath": "/health",
      "env": {}
    }
  },
  "dependencies": {
    "express": "^5.1.0"
  }
}
